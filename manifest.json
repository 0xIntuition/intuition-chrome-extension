{
  "manifest_version": 3,
  "name": "Intuition",
  "version": "1.0",
  "description": "Displays social reputation of the current URL",
"permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
